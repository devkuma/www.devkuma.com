<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>Vue Test</title>
</head>
<body>
  <div id="app">
    <ul>
      <li v-for="(val, key) in list">(Key:{{ key }}) {{ val }}</li>
    </ul>
  </div>
  <script src="https://unpkg.com/vue/dist/vue.js"></script>
  <script>
    new Vue({
      el: '#app',
      data: {
        list: {
          a: '햄버거',
          b: '불고기',
          c: '파스타'
        }
      }
    })
  </script>
</body>
</html>
